@model demoTraining.Models.Staff

@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Profile</h2>

<div>
    <h4>@Html.DisplayFor(model => model.StaffName)</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Staff Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StaffName)
        </dd>

        <dt>
            DoB
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DOB)
        </dd>

        <dt>
            Staff Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StaffEmail)
        </dd>

        <dt>
            Staff Phone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StaffPhone)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.StaffID }, new { @class = "btn btn-warning" }) |
    @Html.ActionLink("Back to List", "Index",null, new { @class = "btn btn-default"})
</p>
